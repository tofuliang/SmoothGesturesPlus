{
   "background": {
      "scripts": [ "js/jquery.min.js", "js/sjcl.js", "js/bg.js" ]
   },
   "content_scripts": [ {
      "all_frames": true,
      "js": [ "js/gestures.js" ],
      "match_about_blank": true,
      "matches": [ "\u003Call_urls>" ],
      "run_at": "document_start"
   }, {
      "all_frames": true,
      "js": [ "js/gestures.js", "js/jquery.min.js" ],
      "match_about_blank": true,
      "matches": [ "\u003Call_urls>" ],
      "run_at": "document_end"
   } ],
   "default_locale": "zh",
   "description": "__MSG_description__",
   "icons": {
      "128": "img/icon128.png",
      "16": "img/icon16.png",
      "32": "img/icon32.png",
      "48": "img/icon48.png"
   },
   "manifest_version": 2,
   "minimum_chrome_version": "20",
   "name": "Smooth Gestures Plus",
   "optional_permissions": [ "nativeMessaging", "bookmarks" ],
   "options_page": "options.html",
   "page_action": {
      "default_icon": "img/pageaction.png",
      "default_popup": "popup.html"
   },
   "permissions": [ "tabs", "storage", "notifications", "idle", "unlimitedStorage", "\u003Call_urls>" ],
   "short_name": "Smooth Gestures Plus",
   "update_url": "https://raw.githubusercontent.com/tofuliang/SmoothGesturesPlus/master/updates.xml",
   "version": "99.2.8.1"
}
